{
  "name": "Receipt Processing Complete",
  "nodes": [
    {
      "parameters": {
        "path": "E:\\Personal Finance Receipt Sorter\\Personal-Finance-Receipt-Sorter\\inbox",
        "watchForFileChanges": true,
        "watchForFolderChanges": false
      },
      "id": "c1c8b7a5-1234-4567-8901-123456789012",
      "name": "Local File Trigger",
      "type": "n8n-nodes-base.localFileTrigger",
      "typeVersion": 1,
      "position": [240, 300]
    },
    {
      "parameters": {
        "command": "=powershell -Command \"$file = '{{ $json.path }}'; $ext = [System.IO.Path]::GetExtension($file).ToLower(); $outputFile = 'tmp/ocr_text.txt'; if ($ext -eq '.pdf') { python -m ocrmypdf --force-ocr --sidecar '$outputFile' '$file' 'tmp/temp_output.pdf' } elseif ($ext -eq '.txt') { Copy-Item '$file' '$outputFile' } else { & 'C:\\Program Files\\Tesseract-OCR\\tesseract.exe' '$file' 'tmp/ocr_text' -l eng }; if (Test-Path '$outputFile') { Get-Content '$outputFile' -Raw } else { 'OCR processing failed for: ' + $file }\""
      },
      "id": "c2c8b7a5-1234-4567-8901-123456789012",
      "name": "Execute Command",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [440, 300]
    },
    {
      "parameters": {
        "resource": "chat",
        "operation": "create",
        "model": "gpt-4o-mini",
        "messages": {
          "values": [
            {
              "role": "system",
              "content": "You are a receipt parsing assistant. Extract key information from receipt text and return it in the specified JSON format."
            },
            {
              "role": "user",
              "content": "=Extract the following fields from this receipt OCR text:\n\n{{ $json.stdout }}"
            }
          ]
        },
        "functions": {
          "values": [
            {
              "name": "parse_receipt",
              "description": "Extract key data from a purchase receipt",
              "parameters": {
                "type": "object",
                "properties": {
                  "vendor": {
                    "type": "string",
                    "description": "The store or vendor name"
                  },
                  "date": {
                    "type": "string",
                    "format": "date",
                    "description": "Purchase date in YYYY-MM-DD format"
                  },
                  "total": {
                    "type": "number",
                    "description": "Total amount paid"
                  },
                  "tax": {
                    "type": "number",
                    "description": "Tax amount"
                  },
                  "currency": {
                    "type": "string",
                    "default": "USD",
                    "description": "Currency code"
                  },
                  "payment_method": {
                    "type": "string",
                    "description": "Payment method used"
                  },
                  "category": {
                    "type": "string",
                    "enum": ["Groceries", "Dining", "Travel", "Utilities", "Shopping", "Gas", "Healthcare", "Entertainment", "Other"],
                    "description": "Spending category"
                  },
                  "items": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "name": {"type": "string"},
                        "price": {"type": "number"},
                        "quantity": {"type": "number", "default": 1}
                      }
                    }
                  }
                },
                "required": ["vendor", "date", "total", "category"]
              }
            }
          ]
        },
        "functionCall": "parse_receipt"
      },
      "id": "c3c8b7a5-1234-4567-8901-123456789012",
      "name": "Message a model",
      "type": "n8n-nodes-base.openAi",
      "typeVersion": 1,
      "position": [640, 300]
    },
    {
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "id": "receipt_data",
              "name": "receipt_data",
              "value": "={{ JSON.parse($json.choices[0].message.function_call.arguments) }}",
              "type": "object"
            },
            {
              "id": "source_file",
              "name": "source_file",
              "value": "={{ $('Local File Trigger').item.json.path }}",
              "type": "string"
            },
            {
              "id": "ocr_text",
              "name": "ocr_text",
              "value": "={{ $('Execute Command').item.json.stdout }}",
              "type": "string"
            }
          ]
        }
      },
      "id": "c4c8b7a5-1234-4567-8901-123456789012",
      "name": "Set",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3,
      "position": [840, 300]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "http://localhost:3001/store-receipt",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        },
        "sendBody": true,
        "contentType": "json",
        "body": "={{ $json.receipt_data }}",
        "options": {}
      },
      "id": "c5c8b7a5-1234-4567-8901-123456789012",
      "name": "HTTP Request",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4,
